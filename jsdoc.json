{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": ["./src"],                       
        "exclude": ["node_modules"]        
    },               
    "opts": {
        "destination": "./jsdoc/",     
        "recurse": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "recurseDepth": 10
}